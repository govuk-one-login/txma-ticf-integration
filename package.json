{
  "name": "di-txma-ticf-integration",
  "description": "System to automate the retrieval of audit data via Zendesk integration",
  "repository": {
    "type": "git",
    "url": "https://github.com/alphagov/di-txma-ticf-integration"
  },
  "author": "GDS",
  "license": "MIT",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "scripts": {
    "build": "yarn bundleApiSpecs && tsc --noEmit && ts-node ./esbuild.config.ts",
    "bundleApiSpecs": "mkdir -p dist && cp -f open-api-specification.yaml dist/open-api-specification.yaml",
    "createTestTicket": "FIXED_RECIPIENT_EMAIL=$0 FIXED_DATA_REQUEST_DATE=$1 FIXED_SUBJECT_LINE=$2 OVERRIDE_EVENT_IDS=$3 DATA_PATHS=$4 ts-node scripts/createAndApproveZendeskTicket.ts",
    "lint": "prettier . --check || exit 1 ; eslint . --max-warnings=0",
    "lint:fix": "prettier . --write ; eslint . --fix",
    "postinstall": "husky install",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "setupDevStackSecrets": "ts-node scripts/setupDevStackSecrets.ts",
    "test": "jest -c jest.config.ts",
    "test:cov": "jest -c jest.config.ts --coverage",
    "test:e2e": "jest -c tests/e2e-tests/jest.e2e.config.ts --expand",
    "test:e2e:dev": "jest --setupFiles '<rootDir>/.env' -c tests/e2e-tests/jest.e2e.config.ts --expand",
    "test:integration": "jest -c tests/integration-tests/jest.integration.config.ts --expand",
    "test:integration:dev": "jest --setupFiles '<rootDir>/.env' -c tests/integration-tests/jest.integration.config.ts --expand",
    "validRecipientsManager": "yarn ts-node scripts/validRecipientsManager.ts",
    "sendManualQueryResults": "yarn ts-node scripts/sendManualRequestResults.ts"
  },
  "dependencies": {
    "@aws-crypto/decrypt-node": "4.0.0",
    "@aws-crypto/kms-keyring-node": "4.0.0",
    "@aws-crypto/material-management": "4.0.0",
    "@aws-lambda-powertools/logger": "1.12.1",
    "@aws-sdk/client-athena": "3.388.0",
    "@aws-sdk/client-dynamodb": "3.388.0",
    "@aws-sdk/client-s3": "3.388.0",
    "@aws-sdk/client-s3-control": "3.388.0",
    "@aws-sdk/client-secrets-manager": "3.388.0",
    "@aws-sdk/client-sns": "3.388.0",
    "@aws-sdk/client-sqs": "3.388.0",
    "@aws-sdk/util-endpoints": "3.387.0",
    "notifications-node-client": "7.0.3"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.388.0",
    "@aws-sdk/client-cloudwatch-logs": "3.388.0",
    "@aws-sdk/client-lambda": "3.388.0",
    "@aws-sdk/client-ssm": "3.388.0",
    "@types/aws-lambda": "8.10.119",
    "@types/jest": "29.5.3",
    "@types/jest-when": "3.5.2",
    "@types/node": "18.17.3",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "aws-sdk-client-mock": "3.0.0",
    "aws-sdk-client-mock-jest": "3.0.0",
    "aws-testing-library": "4.0.6",
    "axios": "1.4.0",
    "commander": "11.0.0",
    "csv-string": "4.1.1",
    "esbuild": "0.19.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.3",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-extended": "4.0.0",
    "jest-junit": "16.0.0",
    "jest-when": "3.5.2",
    "lint-staged": "13.2.3",
    "node-html-parser": "6.1.5",
    "prettier": "2.8.8",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "yaml-cfn": "0.3.2"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  }
}
