{
  "name": "di-txma-ticf-integration",
  "description": "System to automate the retrieval of audit data via Zendesk integration",
  "repository": {
    "type": "git",
    "url": "https://github.com/alphagov/di-txma-ticf-integration"
  },
  "author": "GDS",
  "license": "MIT",
  "packageManager": "yarn@3.2.4",
  "private": true,
  "scripts": {
    "allure": "./tests/run-allure.sh",
    "allure:stop": "docker-compose -f tests/docker-compose.allure.yaml down",
    "allure:report": "cp tests/reports/allure-reports/emailable-report-allure-docker-service.html tests/reports/TxMA-Team2-Test-Report-$(date +%d-%m-%Y).html",
    "build": "tsc --noEmit && ts-node ./esbuild.config.ts",
    "createTestTicket": "FIXED_RECIPIENT_EMAIL=$0 FIXED_DATA_REQUEST_DATE=$1 FIXED_SUBJECT_LINE=$2 OVERRIDE_EVENT_IDS=$3 DATA_PATHS=$4 ts-node scripts/createAndApproveZendeskTicket.ts",
    "lint": "prettier . --check || exit 1 ; eslint . --max-warnings=0",
    "lint:fix": "prettier . --write ; eslint . --fix",
    "postinstall": "husky install",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "test": "jest -c jest.config.ts",
    "test:cov": "jest -c jest.config.ts --coverage",
    "test:e2e": "jest -c tests/e2e-tests/jest.e2e.config.ts",
    "test:e2e:dev": "jest --setupFiles '<rootDir>/.env' -c tests/e2e-tests/jest.e2e.config.ts",
    "test:integration": "jest -c tests/integration-tests/jest.integration.config.ts",
    "test:integration:dev": "jest --setupFiles '<rootDir>/.env' -c tests/integration-tests/jest.integration.config.ts"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "1.5.0",
    "@aws-sdk/client-athena": "3.163.0",
    "@aws-sdk/client-dynamodb": "3.163.0",
    "@aws-sdk/client-glue": "3.163.0",
    "@aws-sdk/client-s3": "3.163.0",
    "@aws-sdk/client-s3-control": "3.163.0",
    "@aws-sdk/client-secrets-manager": "3.163.0",
    "@aws-sdk/client-sqs": "3.267.0",
    "notifications-node-client": "5.1.1"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.163.0",
    "@aws-sdk/client-cloudwatch-logs": "3.163.0",
    "@aws-sdk/client-lambda": "3.163.0",
    "@aws-sdk/client-ssm": "3.163.0",
    "@jest/types": "28.1.3",
    "@types/aws-lambda": "8.10.102",
    "@types/jest": "28.1.7",
    "@types/jest-when": "3.5.2",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "aws-sdk-client-mock": "2.0.0",
    "aws-sdk-client-mock-jest": "2.0.0",
    "aws-testing-library": "2.1.5",
    "axios": "0.27.2",
    "csv-string": "4.1.1",
    "esbuild": "0.15.12",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.3",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-extended": "3.1.0",
    "jest-junit": "14.0.1",
    "jest-when": "3.5.1",
    "lint-staged": "13.0.3",
    "node-html-parser": "6.1.1",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "yaml-cfn": "0.3.1"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  }
}
